defines = -D__OHOS__ -D_GNU_SOURCE -DDDS_HAS_LIFESPAN=1 -DDDS_HAS_DEADLINE_MISSED=1 -DDDS_HAS_TYPE_DISCOVERY=1 -DDDS_HAS_TOPIC_DISCOVERY=1
include_dirs = -I../../src/eclipse-cyclonedds/cyclonedds/src/core/ddsc/include -I../../src/eclipse-cyclonedds/cyclonedds/src/core/ddsi/include -I../../src/eclipse-cyclonedds/cyclonedds/stubs -I../../src/eclipse-cyclonedds/cyclonedds/src/ddsrt/include -Igen/src/eclipse-cyclonedds/cyclonedds/cyclonedds/include
cflags = -Wall -Wextra -Wno-unused-parameter -fPIC
cflags_c = -std=c11
target_output_name = minimal_test

build obj/test/cross_device/minimal_test.minimal_test.o: cc ../../test/cross_device/minimal_test.c || obj/src/eclipse-cyclonedds/cyclonedds/cyclone_config_gen.stamp
  source_file_part = minimal_test.c
  source_name_part = minimal_test

build tests/minimal_test: link obj/test/cross_device/minimal_test.minimal_test.o ./libddsc.so
  ldflags =
  libs = -lc -lm -ldl -lpthread
  frameworks =
  swiftmodules =
  output_extension = 
  output_dir = tests
